    #contentBox {
        margin-left: 50px;
    }

    /* fallback */


    .material-icons {
        font-family: 'Material Icons';
        font-weight: normal;
        font-style: normal;
        font-size: 20px; /* Preferred icon size */
        display: inline-block;
        line-height: 1;
        text-transform: none;
        letter-spacing: normal;
        word-wrap: normal;
        white-space: nowrap;
        direction: ltr;
        /* Support for all WebKit browsers. */
        -webkit-font-smoothing: antialiased;
        /* Support for Safari and Chrome. */
        text-rendering: optimizeLegibility;
        /* Support for Firefox. */
        -moz-osx-font-smoothing: grayscale;
        /* Support for IE. */
        font-feature-settings: 'liga';
    }

    /*JEXCEL*/

    .jexcel_toolbar i.jexcel_toolbar_item {
        width: 28px;
        height: 28px;
        padding: 4px;
    }

    .material-icons {
        font-size: 20px;
    }

    .jexcel thead tr:first-child td:nth-child(n+3) {
        background-color: #A9D08E;
        color: #000;
    }

    .jexcel thead tr:nth-child(3) td {
        white-space: normal;
        font-size: 12px;
        font-weight: bold;
    }

    .icon-ctm {
        padding: 5px;
        font-size: 16px;
